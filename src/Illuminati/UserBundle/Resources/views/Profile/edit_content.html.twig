{% trans_default_domain 'FOSUserBundle' %}

<h2 class="page-header">{{ 'profile.edit.title'|trans }}</h2>

<form action="{{ path('fos_user_profile_edit') }}" {{ form_enctype(form) }} method="POST" class="col-md-6 col-md-offset-3">
    {{ form_widget(form) }}
    <div>
        <input class="btn btn-primary" type="submit" value="{{ 'profile.edit.submit'|trans }}" />
    </div>
</form>
